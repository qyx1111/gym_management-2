{
  "name": "gym-management-system",
  "version": "1.0.0",
  "description": "NUAA健身房会员管理系统",
  "scripts": {
    "dev": "cd frontend2 && npm run dev",
    "build": "cd frontend2 && npm run build",
    "preview": "cd frontend2 && npm run preview",
    "install-frontend": "cd frontend2 && npm install",
    "install-backend": "cd backend && npm install",
    "install-all": "npm run install-frontend && npm run install-backend",
    "start-frontend": "cd frontend2 && npm run dev",
    "start-backend": "cd backend && npm start",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": ["gym", "management", "vue", "node"],
  "author": "NUAA",
  "license": "MIT"
}
